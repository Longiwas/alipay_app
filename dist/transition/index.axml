<view class='transition-index {{className}}' onChildRef='saveChildRef0'>
  <view a:if='{{ inited }}' class='van-transition {{customClass}} {{ classes }}' style="-webkit-transition-duration:{{ currentDuration }}ms; transition-duration:{{ currentDuration }}ms; {{ display ? '' : 'display: none;' }} {{ customStyle }}" onTransitionend='onTransitionEnd'>
    <slot>
    </slot>
  </view>
</view>