<view class='progress-index {{className}}' onChildRef='saveChildRef0'>
  <view class='van-progress {{customClass}}'>
    <view class="van-progress__portion {{ showPivot && text ? 'van-progress__portion--with-pivot' : '' }}" style='{{ portionStyle }}'>
      <view a:if='{{ showPivot && text }}' style='{{ pivotStyle }}' class='van-progress__pivot'>
        {{ text }}
      </view>
    </view>
  </view>
</view>