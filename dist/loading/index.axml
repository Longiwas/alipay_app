<view class='loading-index {{className}}' onChildRef='saveChildRef0'>
  <view class='van-loading {{customClass}}' style='width: {{ size }}; height: {{ size }}'>
    <view class='van-loading__spinner van-loading__spinner--{{ type }}' style='color: {{ color }};'>
      <view a:if="{{ type === 'spinner' }}" a:for='item in 12' a:key='index' class='van-loading__dot' ref-numbers='item in 12'>
      </view>
    </view>
  </view>
</view>