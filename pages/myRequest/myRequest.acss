.selected {
    color: #9259ff;
    border-bottom: 2px solid #9259ff;
}
.tabpage {
    position: relative;
    padding-top: 32px;
}
.deactivate {
    display: none;
}
.scroll-view {
    height: calc(100% + 32px);
    transform: translateY(-32px);
}
.pulldown {
    height: 40px;
    width: 100%;
    line-height: 40px;
    text-align: center;
}
.petItem {
    width: 100%;
    flex-direction: row;
    border-bottom: 1px solid #c2c2c2;
}
.petImg {
    border-radius: 10%;
    margin: 35rpx;
}
.petInfo {
    flex-direction: column;
}
.petInfoItem {
    height: 25%;
    width: 100%;
    display: flex;
    align-items: center;
    flex-direction: row;
}
.petName {
    font-weight: bold;
    overflow: hidden;
    line-height: 100%;
}
.petLocation {
    color: #9b9b9b;
}
.locationImg {
    height: 30rpx;
    width: 28rpx;
    margin-right: 8rpx;
}
.tags {
    color: #9259ff;
    border: 1px solid rgba(146, 89, 255, 0.6);
    padding: 4rpx 20rpx;
    border-radius: 30rpx;
    font-size: 24rpx;
    text-align: center;
}
.op06 {
    opacity: 0.6;
}
.btn_9057FF {
    background: rgba(144, 87, 255, 1);
    border-radius: 30rpx;
    font-size: 24rpx;
    font-weight: 400;
    color: white;
    line-height: 24rpx;
    padding: 12rpx 28rpx;
}
.btn_FF5998 {
    background: rgba(255, 89, 152, 1);
    border-radius: 24rpx;
    font-size: 24rpx;
