.failed-popup {
    width: 100%;
}
.failed-popup-title {
    width: 100%;
    margin: 16px 0 16px 0;
    font-weight: 400;
    display: flex;
    justify-content: center;
    text-align: center;
    font-size: 18px;
    color: #5b5b5b;
}
.failed-popup-content {
    margin: 16px 35px 16px 35px;
    font-size: 14px;
    color: #9b9b9b;
}
.failed-popup-button-view {
    margin: 16px 0 16px 0;
    display: flex;
    flex-direction: column;
    justify-content: center;
    align-items: center;
}
.failed-popup-button {
    height: 50px;
    width: 90%;
    margin: 8px 0 8px 0;
    border-radius: 50px;
    border: 1px solid #9057ff;
    color: #9057ff;
    display: flex;
    justify-content: center;
    text-align: center;
}

.failed-popup-other-button {
    height: 50px;
    width: 90%;
    margin: 8px 0 8px 0;
    border-radius: 50px;
    border: 1px solid #9057ff;
    background-color: #9057ff;
    color: white;
    display: flex;
    justify-content: center;
    text-align: center;
}
.cancelPopup {
    width: 100%;
    height: 250px;
    font-size: 16px;
}
.cancelPopupInfo {
    height: 33%;
    display: flex;
    justify-content: center;
    align-items: center;
    text-align: center;
}
.cancelPopupConfirm {
    width: 600rpx;
    height: 100rpx;
    background-color: #9057ff;
    font-size: 20px;
    border-radius: 30px;
    color: white;
}
.cancelPopupCancel {
    width: 600rpx;
    height: 100rpx;
    background-color: white;
    font-size: 20px;
    border-radius: 30px;
    color: #9b9b9b;
    border: 1px solid #9b9b9b;
}
