.selected {
    color: #9259ff;
    border-bottom: 2px solid #9259ff;
}
.tabpage {
    margin-top: 32px;
}
.deactivate {
    display: none;
}
.scroll-view {
    height: calc(100% + 32px);
    transform: translateY(-32px);
}
.pulldown {
    height: 40px;
    width: 100%;
    line-height: 40px;
    text-align: center;
}
.petItem {
    width: 100%;
    flex-direction: row;
    border-bottom: 1px solid #c2c2c2;
}
.petImg {
    border-radius: 10%;
    margin: 30rpx;
    height: 260rpx;
    width: 260rpx;
}
.petInfo {
    flex-direction: column;
}
.petInfoItem {
    height: 25%;
    display: flex;
    align-items: center;
    flex-direction: row;
}
.petName {
    font-weight: bold;
    overflow: hidden;
    line-height: 100%;
}
.petLocation {
    color: #9b9b9b;
}
.petPowerAvatar {
    height: 40rpx;
